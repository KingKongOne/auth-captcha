{
    "name": "asundust/auth-captcha",
    "description": "Sliding captcha for laravel-admin auth / laravel-admin登陆 滑动验证插件",
    "type": "library",
    "keywords": [
        "laravel-admin",
        "extension",
        "sliding-captcha",
        "auth-captcha"
    ],
    "homepage": "https://github.com/asundust/auth-captcha",
    "license": "MIT",
    "authors": [
        {
            "name": "asundust",
            "email": "asundust@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "encore/laravel-admin": "~1.6",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Asundust\\AuthCaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asundust\\AuthCaptcha\\AuthCaptchaServiceProvider"
            ]
        }
    }
}
